{{page-header title=title 
              icon=icon 
              subTitle=subTitle 
              }}
    <div class="did-you-mean well">
        {{#each model.didYouMean as |group|}}
            <div>
                {{#if group.items}}
                    <strong>{{t group.name}}</strong>
                    {{#each group.items as |item|}}
                        {{#link-to group.route item.id}}<i class="fa fa-{{group.icon}}"></i> {{item.name}}{{/link-to}}
                    {{/each}}
                {{/if}}
            </div>
        {{else}}
            {{t 'dig.notFound'}} {{#link-to 'query' class="btn btn-success"}}<i class="fa fa-tags"></i> {{t 'queryOptions.deep'}}{{/link-to}}
        {{/each}}        
    </div>                    
{{partial "playlist"}}
