{{yield}}
  <div class="modal fade" id="licenseInfoPopup" tabindex="-1" role="dialog" aria-labelledby="licenseInfoPopupLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
          <h4 class="modal-title">{{licenseInfo.title}}</h4>
        </div>
        <div class="modal-body">
            {{partial "licenses-partial"}}
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal">{{t 'tbPopup.close'}}</button>
        </div>
      </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
  </div><!-- /.modal -->


